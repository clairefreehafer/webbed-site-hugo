{{ range $ingredientCategory, $ingredientTypes := .Site.Data.recipes.ingredients }}
  {{ range $ingredientType, $ingredients := $ingredientTypes }}
    {{ $params := dict "ingredientType" $ingredientType }}
    {{ range $ingredients }}
      {{ if . }}
        {{ $page := dict
          "kind" "term"
          "params" $params
          "path" .
          "title" .
        }}
        {{ $.AddPage $page }}
      {{ end }}
    {{ end }}
  {{ end }}
{{ end }}
